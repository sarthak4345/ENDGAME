		SYMTAB = new LinkedHashMap<>();
		LITTAB = new ArrayList<>();
		POOLTAB = new ArrayList<>();
		lc=0;
		POOLTAB.add(0);
		mnemonicTable = new MnemonicTable();
	}
	
	public static void main(String[] args) throws IOException {
		
		PassOne p = new PassOne();
		
		p.parseFile();

	}

