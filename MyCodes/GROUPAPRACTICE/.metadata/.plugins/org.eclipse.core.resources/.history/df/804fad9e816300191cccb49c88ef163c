		br = new BufferedReader(new FileReader("input.asm"));
		BufferedWriter bw = new BufferedWriter(new FileWriter("IC.txt"));
		String line,code;
		
		while((line = br.readLine())!=null) {
			
			String parts[] = line.split("\\s+");
			
			
			
		}
	}

}
